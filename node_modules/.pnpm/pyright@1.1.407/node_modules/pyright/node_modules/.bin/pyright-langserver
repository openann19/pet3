#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ben/Downloads/PETSPARK/node_modules/.pnpm/pyright@1.1.407/node_modules/pyright/node_modules:/home/ben/Downloads/PETSPARK/node_modules/.pnpm/pyright@1.1.407/node_modules:/home/ben/Downloads/PETSPARK/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/ben/Downloads/PETSPARK/node_modules/.pnpm/pyright@1.1.407/node_modules/pyright/node_modules:/home/ben/Downloads/PETSPARK/node_modules/.pnpm/pyright@1.1.407/node_modules:/home/ben/Downloads/PETSPARK/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../langserver.index.js" "$@"
else
  exec node  "$basedir/../../langserver.index.js" "$@"
fi
